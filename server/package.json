{
  "name": "server-express-freelancer",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm build  --prefix client",
    "client": "npm start  --prefix client",
    "server": "set PORT=3001 && node index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "aws-sdk": "^2.224.1",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "cluster": "^0.7.7",
    "concurrently": "^3.5.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "jade": "~1.11.0",
    "mocha": "^5.0.4",
    "mongodb": "^3.0.5",
    "mongoose": "^5.0.14",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.3",
    "serve-favicon": "~2.4.5"
  }
}
